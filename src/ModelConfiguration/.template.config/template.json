{
  "$schema": "http://json.schemastore.org/template",
  "author": "API Template Pack",
  "name": "Model Configuration generation template",
  "description": "Generates a model configuration for database projects",
  "identity": "ModelConfig",
  "shortName": "modelconfig",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "ModelConfiguration",
  "symbols": {
    "root": {
      "type": "parameter",
      "replaces": "placeHolder",
      "dataType": "string",
      "defaultValue": "Threenine"
    },
    "model": {
      "type": "parameter",
      "replaces": "Model",
      "dataType": "string",
      "defaultValue": "Model"
    },
    "useMSSQL": {
      "type": "computed",
      "value": "(!usePOSTGRE)"
    },
    "usePOSTGRE": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false"
    },
    "entity": {
      "type": "computed",
      "value": "(!list)"
    },
    "list": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false"
    }
  },
  "classifications": [
    "API Template Pack"
  ]
}