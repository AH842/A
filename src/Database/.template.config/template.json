    "usePOSTGRE": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false"
    }
  },
  "classifications": [
    "webapi", "cqrs", "vertical", "project"
  ]
}

  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
